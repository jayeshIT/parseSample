		<View id="image_user">
			<ImageView id="userimage" />
			<Button id="upload" />
		</View>
		<ScrollView id="AddMyprofile">
			<TextField id="txtFullName" class="textfields"/>
			<View class="divider" id="divider"/>
			<TextField id="txtUserName" class="textfields"/>
